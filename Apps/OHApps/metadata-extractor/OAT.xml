        </filefilterlist>
        <policylist>
            <policy
                desc="协议相关策略"
                name="projectPolicy">
                <policyitem
                    desc="原开源项目的LICENSE, 类似于 MIT licenses, 符合开源要求"
                    filefilter="defaultPolicyFilter"
                    group="defaultGroup"
                    name="MIT"
                    path=".*"
                    type="license"/>
                <policyitem
                    desc="原开源项目的LICENSE, 类似于MIT licenses, 符合开源要求"
                    filefilter="defaultPolicyFilter"
                    group="defaultGroup"
                    name="Copyright (c) 2022 Huawei Device Co., Ltd."
                    path=".*"
                    rule="may"
                    type="copyright"/>
            </policy>
        </policylist>
    </oatconfig>
