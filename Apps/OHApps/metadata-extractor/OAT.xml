                <filteritem
                    desc="hvigor配置文件，DevEco Studio自动生成，不手动修改"
                    name="hvigorw.bat"
                    type="filename"/>
                <filteritem
                    desc="hvigor配置文件，DevEco Studio自动生成，不手动修改"
                    name="hvigor-wrapper.js"
                    type="filename"/>
                <filteritem
                    desc="hvigor配置文件，DevEco Studio自动生成，不手动修改"
                    name="*.json"
                    type="filename"/>
                <filteritem
                desc="测试辅助脚本"
                name="*.bat"
                type="filename"/>
            </filefilter>
            <filefilter
                desc="Filters for compatibility，license header policies"
                name="defaultPolicyFilter">
